<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Abn_ID">Abn_Nom</label>
  <div class="col-sm-7">
    <select class="form-control" name="Abn_ID">
      <% for (const abn of availableAbns) { %>
        <option value="<%= abn.Abn_ID %>"><%= abn.Nom %> <%= abn.Prenom %></option>
      <% } %>
    </select>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Livre_ID">Livre_Titre</label>
  <div class="col-sm-10">
    <select class="form-control" name="Livre_ID">
      <% for (const livre of availableLivres) { %>
        <option value="<%= livre.Livre_ID %>"><%= livre.Titre %></option>
      <% } %>
    </select>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="DatePret">DatePret</label>
  <div class="col-sm-7">
    <input class="form-control" name="DatePret" value="<%= model.DatePret %>" />
  </div>
</div>
