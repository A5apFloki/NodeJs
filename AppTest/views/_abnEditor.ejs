<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Nom">Nom</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="Nom" value="<%= model.Nom %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Prenom">Prenom</label>
    <div class="col-sm-7">
      <input class="form-control" name="Prenom" value="<%= model.Prenom %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Commentaires">Commentaires</label>
    <div class="col-sm-10">
      <textarea class="form-control" cols="20" name="Commentaires" maxlength="32000" rows="7"><%= model.Commentaires %></textarea>
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Numero">Numero</label>
    <div class="col-sm-7">
      <input class="form-control" name="Numero" value="<%= model.Numero %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Addresse">Addresse</label>
    <div class="col-sm-7">
      <input class="form-control" name="Addresse" value="<%= model.Addresse %>" />
    </div>
  </div>