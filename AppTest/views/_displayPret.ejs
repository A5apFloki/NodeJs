<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Pret_ID">Pret_ID</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="Pret_ID" value="<%= model.Pret_ID %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Abn_ID">Abn_ID</label>
    <div class="col-sm-7">
      <input class="form-control" name="Abn_ID" value="<%= model.Abn_ID %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Livre_ID">Livre_ID</label>
    <div class="col-sm-10">
      <textarea class="form-control" cols="20" name="Livre_ID" maxlength="1000" rows="7"><%= model.Livre_ID %></textarea>
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="DatePret">DatePret</label>
    <div class="col-sm-7">
      <input class="form-control" name="DatePret" value="<%= model.DatePret %>" />
    </div>
  </div>
  